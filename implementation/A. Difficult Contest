
    static void A(int t) {

        Arrays.sort(x);

        for (int i = 0, j = n - 1; i < j; ++i, --j) {

            char temp = x[i];
            x[i] = x[j];
            x[j] = temp;

        }

        ans.append(new String(x));

        if (t != testCases) {

            ans.append("\n");

        }

    }
