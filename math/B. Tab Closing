
    static void B(int t) {

        if (equal(X, Y)) {

            ans.append("1");

        } else if (isSmaller(N + "", (X / Y) + "")) {

            ans.append("1");

        } else if (equal(N, (X / Y))) {

            ans.append("1");

        } else {

            ans.append("2");

        }

        if (t != testCases) {

            ans.append("\n");

        }

    }
