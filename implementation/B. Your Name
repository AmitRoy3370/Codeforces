
    static void B(int t) {

        int count1[] = new int[26];
        int count2[] = new int[26];

        for (char i : x) {

            count1[i - 'a']++;

        }

        for (char i : y) {

            count2[i - 'a']++;

        }

        yes = true;

        for (int i = 0; i < 26; ++i) {

            if (count1[i] != count2[i]) {

                yes = false;
                break;

            }

        }

        if (yes) {

            ans.append("YES");

        } else {

            ans.append("NO");

        }

        if (t != testCases) {

            ans.append("\n");

        }

    }
