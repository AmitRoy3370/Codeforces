import java.util.*;
import java.math.*;
public class Hie
{
public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int m=sc.nextInt();
char a[][]=new char[n][m];
int r[]=new int[10];
int c[]=new int[10];
for(int i=0;i<n;i++)
{
String d=sc.next();
for(int j=0;j<m;j++){
a[i][j]=d.charAt(j);
if(a[i][j]=='S')
{
r[i]=1;
c[j]=1;
}
}}

int s=0;
for(int i=0;i<n;i++)
{
for(int j=0;j<m;j++){
if(r[i]==0 || c[j]==0)
s++;
}}
		
System.out.println(s);
}
}

