 static void A(int t) {
 
        A = new long[n][n];
 
        sum = 1;
 
        for (int i = 0; i < n; ++i) {
 
            for (int j = 0; j < n; ++j) {
 
                A[i][j] = sum++;
 
            }
 
        }
 
        sum = 0L;
 
        for (int i = 0; i < n; ++i) {
 
            for (int j = 0; j < n; ++j) {
 
                long tempSum = A[i][j];
 
                if (i - 1 >= 0) {
 
                    tempSum += A[i - 1][j];
 
                }
 
                if (i + 1 < n) {
 
                    tempSum += A[i + 1][j];
 
                }
 
                if (j - 1 >= 0) {
 
                    tempSum += A[i][j - 1];
 
                }
 
                if (j + 1 < n) {
 
                    tempSum += A[i][j + 1];
 
                }
 
                sum = Math.max(sum, tempSum);
 
            }
 
        }
 
        ans.append(sum);
 
        if (t != testCases) {
 
            ans.append("\n");
 
        }
 
    }
 
